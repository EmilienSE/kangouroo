<div class="container" style="margin-top: 70px;">
    <div class="row">
        <div class="col-12">
            <h1>Ajouter un utilisateur</h1>
        </div>
        <div class="col-12">
            <form method="post" action="/add/">
                <div class="row">
                    <div class="col form-group">
                        <input type="email" class="form-control" placeholder="Votre adresse email" name="email">
                    </div>
                    <div class="col form-group">
                        <input type="text" class="form-control" placeholder="Votre prénom" name="first_name">
                    </div>
                    <div class="col form-group">
                        <input type="text" class="form-control" placeholder="Votre nom" name="last_name">
                    </div>
                    <div class="col form-group">
                        <input type="text" class="form-control" placeholder="Votre numéro de téléphone" name="phone">
                    </div>
                    <div class="col form-group">
                        <input type="password" class="form-control" placeholder="Password" name="password">
                    </div>
                </div>
                <div class="row mt-2">
                    <div class="col">
                        <button type="submit" class="btn btn-primary">Valider</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
